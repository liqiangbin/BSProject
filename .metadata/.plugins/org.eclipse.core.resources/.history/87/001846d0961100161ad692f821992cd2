		List<Suggest> suggestList=suggestService.
		return "/manager/suggest";
    } 
	@RequestMapping("/updateSuggest")  
	 public String updateSuggest(HttpServletRequest request,Model model,Manager manager,HttpSession session) throws ParseException{  
		return "/manager/suggest";
   } 

